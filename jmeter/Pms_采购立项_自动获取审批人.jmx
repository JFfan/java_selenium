              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.io.BufferedReader;
import java.io.FileInputStream;
import java.io.InputStream;
import java.io.InputStreamReader;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

InputStream is = new FileInputStream(&quot;H:\\jemter\\auditing.txt&quot;);
BufferedReader reader = new BufferedReader(new InputStreamReader(is,&quot;utf-8&quot;));
String line;
String pattterns=vars.get(&quot;patttern&quot;);
String patttern=pattterns;
int tmp= ctx.getThreadNum();
int num = tmp+1;
line = reader.readLine(); // 读取第一行
while (line != null) { // 如果 line 为空说明读完了
    Pattern pcallType = Pattern.compile(patttern);
	Matcher mcallType = pcallType.matcher(line);
	if (mcallType.find()) {
