        <hashTree>
          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="参数化提交立项表单" enabled="true">
            <boolProp name="resetInterpreter">true</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import java.io.BufferedReader;
import java.io.FileInputStream;
import java.io.InputStream;
import java.io.InputStreamReader;
import java.util.regex.Matcher;
import java.util.regex.Pattern;
import org.apache.jmeter.services.FileServer;
String file=FileServer.getFileServer().getBaseDir();
InputStream is = new FileInputStream(file+&quot;\\立项.txt&quot;);
BufferedReader reader = new BufferedReader(new InputStreamReader(is,&quot;utf-8&quot;));
String line;
String name=vars.get(&quot;name&quot;);
line = reader.readLine(); // 读取第一行
while (line != null) { // 如果 line 为空说明读完了
	String patt=name+&quot;,(.*),(.*),(.*),(.*),(.*),(.*),(.*),(.*),(.*)&quot;;
     Pattern pcallType = Pattern.compile(patt);
	Matcher mcallType = pcallType.matcher(line);
	if (mcallType.find()) {
